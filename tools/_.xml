<?xml version="1.0" encoding="UTF-8"?>
<toolSet>
  <tool name="emacs" showInMainMenu="true" showInEditor="true" showInProject="true" showInSearchPopup="true" disabled="false" useConsole="false" synchronizeAfterRun="true">
    <exec>
      <option name="COMMAND" value="emacs" />
      <option name="PARAMETERS" value="$FilePath$" />
      <option name="WORKING_DIRECTORY" value="$FileDir$" />
    </exec>
  </tool>
  <tool name="Regen Gradle Modules" showInMainMenu="true" showInEditor="false" showInProject="false" showInSearchPopup="false" disabled="false" useConsole="true" synchronizeAfterRun="true">
    <exec>
      <option name="COMMAND" value="$USER_HOME$/usr/gradle/bin/gradle" />
      <option name="PARAMETERS" value="ideaModule" />
      <option name="WORKING_DIRECTORY" value="$ProjectFileDir$" />
    </exec>
  </tool>
  <tool name="Regen Gradle Project" showInMainMenu="true" showInEditor="false" showInProject="false" showInSearchPopup="false" disabled="false" useConsole="true" synchronizeAfterRun="true">
    <exec>
      <option name="COMMAND" value="$USER_HOME$/usr/gradle/bin/gradle" />
      <option name="PARAMETERS" value="idea" />
      <option name="WORKING_DIRECTORY" value="$ProjectFileDir$" />
    </exec>
  </tool>
</toolSet>

